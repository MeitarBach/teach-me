<!DOCTYPE html>
<html>
<head>
    <title>TeachMe | Store</title>
	<% include partials/head %>

</head>
	
<body>
<!-- header -->
	<% include partials/admin-header %>
<!-- //header -->

<!-- banner -->
	<div class="banner">
		<div class="container">
			<div class="banner-info animated wow zoomIn" data-wow-delay=".5s">
        		<h3>Learn Everything Anywhere</h3><br>
        		<h4>Expand Your Skills Set And Start Learning</h4><br>
				<div class="wmuSlider example1">
				</div>
			</div>
		</div>
    </div>
					<script src="../javascripts/jquery.wmuSlider.js"></script> 
					<script>
						$('.example1').wmuSlider();         
					</script> 
			</div>
		</div>
	</div>
<!-- //banner -->

<!-- collections -->
<div class="new-collections">
    <div class="container">
        <h3 class="animated wow zoomIn" data-wow-delay=".5s">All Lessons</h3>
        <p class="est animated wow zoomIn" data-wow-delay=".5s">Excepteur sint occaecat cupidatat non proident, sunt in culpa qui officia 
            deserunt mollit anim id est laborum.</p>

        <% for (let i = 0; i < users.length ; i++) { %>
            <div class="new-collections-grids">
               <% for (let j = 0 ; j < users[i].length ; j++){ %>
                <div class="col-md-4">
                    <div class="new-collections-grid1 animated wow slideInUp" data-wow-delay=".5s">
                        <div class="new-collections-grid1-image">
                            <div class="lesson-image"><a href="single.html" class="product-image"><img src="images/guitar-Efi.Jpeg" alt=" " class="img-responsive" /></a>
                            </div>
                                <div class="new-collections-grid1-image-pos">
                                <a href="single.html">Quick View</a>
                            </div>
                            <div class="new-collections-grid1-right">
                            </div>
                        </div>
                        <h4><a href="single.html"><%= users[i][j].title %></a></h4>
                        <p>Instructor: <%= users[i][j].instructor %></p>
                        <p>Time: <%= users[i][j].startTime%> - <%= users[i][j].endTime %></p>
                        <div class="new-collections-grid1-left simpleCart_shelfItem">
                            <p><span class="item_price">$<%= users[i][j].price %></span><a class="item_add" href="#">add to cart </a></p>
                        </div>
                    </div>
                </div>
               <% } %>
            </div>
            <div class="clearfix"> </div>
        <% } %>
    </div>
</div>
<!-- //collections -->

<!-- footer -->
<% include partials/footer %>
<!-- //footer -->
</body>
</html>